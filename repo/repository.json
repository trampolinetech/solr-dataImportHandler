            },
            {
              "url": "mariadb-java-client-2.6.0.jar",
              "sig": "VOIiudpKnF9Z1/dkkzVQ1s4Mjo4blJNi8pDSAt06ag//aU2BL7XMukevoSr0uhDyOsHXMA3rCYaS15b/ciJhCw=="
            },
            {
              "url": "jython-standalone-2.7.3.jar",
